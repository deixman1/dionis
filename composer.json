{
    "name": "project/dionis",
    "description": "dionis project",
    "require": {
        "php": ">=8.1",
        "php-di/php-di": "^6.3",
        "psr/log": "^1.1",
        "psr/http-message": "^1.0",
        "slim/psr7": "*",
        "slim/slim": "^4.7",
        "curl/curl": "^2.3",
        "symfony/var-dumper": "^5.2",
        "ext-curl": "*",
        "monolog/monolog": "*"
    },
    "autoload": {
        "psr-4": {
            "SRC\\": "src/",
            "Config\\": "config/",
            "Logs\\": "logs/",
            "Libs\\": "libs/"
        }
    },
    "require-dev": {
        "codeception/codeception": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*",
      "ext-json": "*"
    }
}
